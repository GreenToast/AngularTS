{
  "name": "AngularTS",
  "version": "0.0.0",
  "authors": [
    "Dennis Neuendorf"
  ],
  "keywords": [
    "Angular",
    "TypeScript",
    "Gulp"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "^2.1.3",
    "angular": "^1.3.15",
    "angular-route": "^1.3.15",
    "bootstrap": "^3.3.4"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap-theme.min.css",
        "dist/fonts/*"
      ]
    },
    "jquery": {
      "main": {
        "development": [
          "dist/jquery.min.js",
          "dist/jquery.min.map"
        ],
        "production": [
          "dist/jquery.min.js"
        ]
      }
    },
    "angular": {
      "main": [
        "angular.min.js"
      ]
    },
    "angular-route": {
      "main": [
        "angular-route.min.js"
      ]
    }
  }
}
